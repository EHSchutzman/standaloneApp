<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<ScrollPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.mainPageController">
   <content>
      <AnchorPane prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label layoutX="430.0" layoutY="118.0" text="No Image" />
            <ProgressBar layoutX="240.0" layoutY="343.0" prefWidth="200.0" progress="0.98" />
            <Label layoutX="173.0" layoutY="345.0" text="Progress: " />
            <Button fx:id="browse_button" layoutX="76.0" layoutY="59.0" mnemonicParsing="false" onMouseClicked="#browseForFile" text="Browse For Image" />
            <ImageView fx:id="label_image" fitHeight="240.0" fitWidth="264.0" layoutX="326.0" layoutY="12.0" pickOnBounds="true" preserveRatio="true" />
            <TextArea fx:id="extraLabelInfo" layoutX="14.0" layoutY="199.0" prefHeight="54.0" prefWidth="304.0" />
            <Label layoutX="14.0" layoutY="126.0" text="Show any Information that is Blown Branded, " />
            <Label layoutX="14.0" layoutY="150.0" text="Or Embossed on the Container Only if " />
            <Label layoutX="16.0" layoutY="173.0" text="It Does NOT Appear on the Labels Affixed Below " />
            <TextField fx:id="signature" layoutX="282.0" layoutY="295.0" prefHeight="23.0" prefWidth="158.0" promptText="Signature (Required)" />
            <Label layoutX="211.0" layoutY="300.0" text="Signature" />
            <Button fx:id="nextButton" layoutX="458.0" layoutY="338.0" mnemonicParsing="false" text="Next" />
            <Button fx:id="backButton" layoutX="107.0" layoutY="338.0" mnemonicParsing="false" text="Back" />
         </children>
      </AnchorPane>
   </content>
</ScrollPane>
